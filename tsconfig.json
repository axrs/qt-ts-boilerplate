{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "outFile": "./built/local/tsc.js",
        "sourceMap": true
    },
    "include": [
        "src/**/*",
        "typings/**/*"
    ],
    "exclude": [
        "**/*.spec.ts"
    ]
}